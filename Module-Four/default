default menu.c32
prompt 0
timeout 300
ONTIMEOUT local

MENU TITLE PXE Menu

LABEL Centos 7
	MENU LABEL Centos 7
	KERNEL menu.c32
	APPEND pxelinux.cfg/Centos7

LABEL Centos 8 Stream
	MENU LABEL Centos 8 Stream
	KERNEL menu.c32
	APPEND pxelinux.cfg/Centos8

LABEL Centos 9 Stream
	MENU LABEL Centos 9 Stream
	KERNEL menu.c32
	APPEND pxelinux.cfg/Centos9

LABEL local
        MENU LABEL Boot local hard drive
        LOCALBOOT 0

